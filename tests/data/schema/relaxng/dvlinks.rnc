default namespace = "http://dfg-viewer.de/"

start = links

links =
   element links {
      reference+,
      presentation?,
      sru?
   }

reference =
   element reference {
      attribute linktext { text }?,
      xsd:anyURI
   }

presentation =
   element presentation {
      xsd:anyURI
   }

sru =
   element sru {
      xsd:anyURI
   }